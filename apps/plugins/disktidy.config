# Disktidy config
# When adding a new file, make sure it's NOT enabled
# WARNING: Be careful when you use custom entries.
# You could accidentally delete important files.
< ALL >: no
< NONE >: no
< Windows >: no
    desktop.ini: no
    Thumbs.db: no
    Recycled/: no
    $RECYCLE.BIN/: no
    System Volume Information/: no
< Mac OS 9 >: no
    FINDER.DAT: no
    Desktop DB: no
    Desktop DF: no
< Mac OSX >: no
    .fseventsd/: no
    .Trashes/: no
    .Spotlight-V100/: no
    .DS_Store: no
    ._*: no
< Linux/BSD >: no
    .Trash-*/: no
    .dolphinview: no
    .d3lphinview: no
< Sansa AMS >: no
    UPGRADE.FIN: no
    MTABLE.SYS: no
    RES_INFO.SYS: no
    SYS_CONF.SYS: no
    MUSIC/: no
    \##MUSIC#/: no
    \##PORT#/: no
    RECORD/: no
    PODCASTS/: no
    AUDIBLE/: no
    AUDIOBOOKS/: no
< Other >: no
